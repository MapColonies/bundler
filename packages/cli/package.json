{
  "name": "@bundler/cli",
  "version": "1.0.0",
  "description": "bundler cli",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "format": "prettier --check . --ignore-path ../../.prettierignore",
    "format:fix": "prettier --write . --ignore-path ../../.prettierignore",
    "prelint:fix": "npm run format:fix",
    "prelint": "npm run format",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json && npm run assets:copy",
    "start": "npm run build && cd dist && node ./index.js",
    "assets:copy": "copyfiles ./package.json dist",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MapColonies/bundler.git"
  },
  "author": "MapColonies",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MapColonies/bundler/issues"
  },
  "dependencies": {
    "@bundler/child-process": "*",
    "@bundler/common": "*",
    "@bundler/core": "*",
    "@bundler/github": "*",
    "@bundler/terminal-ui": "*",
    "@map-colonies/cleanup-registry": "^1.1.0",
    "@map-colonies/read-pkg": "0.0.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "better-ajv-errors": "^1.2.0",
    "chalk": "^4.1.2",
    "lodash": "^4.17.21",
    "pino": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@map-colonies/eslint-config": "^3.1.3",
    "@map-colonies/prettier-config": "0.0.1",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/yargs": "^17.0.18",
    "copyfiles": "^2.4.1",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "jest-create-mock-instance": "^1.1.0",
    "jest-html-reporters": "^2.1.2",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
